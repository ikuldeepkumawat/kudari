{
  "name": "kudarilang-syntax",
  "displayName": "KudariLang Syntax Highlighter",
  "description": "KudariLang - Hindi Programming Language that transpiles to JavaScript",
  "version": "1.0.0",
  "publisher": "ikuldeepkumawat",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "kudari",
    "kudarilang",
    "hindi",
    "programming-language",
    "transpiler",
    "javascript",
    "syntax-highlighting",
    "india"
  ],
  "author": "Kuldeep",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikuldeepkumawat/kudari.git"
  },
  "homepage": "https://github.com/ikuldeepkumawat/kudari#readme",
  "bugs": {
    "url": "https://github.com/ikuldeepkumawat/kudari/issues"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "kudarilang",
        "aliases": ["KudariLang", "kudarilang", "Kudari"],
        "extensions": [".kudari"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/kudarilang-light.png",
          "dark": "./icons/kudarilang-dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "kudarilang",
        "scopeName": "source.kudarilang",
        "path": "./syntaxes/kudarilang.tmLanguage.json"
      }
    ],
    "themes": [
      {
        "label": "KudariLang Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/kudarilang-dark.json"
      }
    ],
    "commands": [
      {
        "command": "kudarilang.runCode",
        "title": "Run KudariLang Code",
        "icon": "$(play)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "kudarilang.runCode",
          "when": "resourceExtname == .kudari",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "kudarilang",
        "path": "./snippets/kudarilang.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "^16.x",
    "typescript": "^4.9.4",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "child_process": "^1.0.2"
  }
}